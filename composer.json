{
    "name": "Composer For the main Software",
    "description": "A Composer file to load all the required php scripts",
    "keywords": ["php", "careMd"],
    "type": "library",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Rayton Kiwelu"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ext-tokenizer": "*",
        "propel/propel": "~2.0@dev",
        "monolog/monolog": ">=1.1.0",
	    "pimple/pimple": "~3.0",
	    "nikic/php-parser": "~1.4",
	    "bcncommerce/json-stream": "0.3.0",
        "nesbot/carbon": "^1.33",
        "phpoffice/phpspreadsheet": "^1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7.0",
        "bcncommerce/stream-wrapper": "dev-master",
        "symfony/phpunit-bridge": "^3.2"
    },

    "autoload": {
        "psr-0": { "Bcn\\Component\\Json\\": "" },
        "classmap": ["propel/CareMd/CareMd/"],
        "psr-4": {"" : ""}
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.4-dev"
        }
    }
}
